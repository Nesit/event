<% if logged_in? %>
	<%= link_to "Профиль", edit_user_path, class: 'profile-link' %> | 
	<%= link_to "Выйти", user_session_path, 'data-method' => :delete, class: 'logout-link' %>
<% else %>
	<%= link_to "Войти", '#', class: 'login-link' %> |
	<%= link_to "Зарегистрироваться", '#', class: 'register-link' %>
<% end %>